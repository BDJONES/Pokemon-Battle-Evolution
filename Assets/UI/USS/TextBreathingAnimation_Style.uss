.breathingText {
    color: rgb(255, 255, 255);
    transition-duration: 0.5s;
    transition-property: all;
    transition-timing-function: ease;
    transition-delay: 0s;
    font-size: 30px;
}

.breathingText:hover {
    color: rgba(255, 255, 255, 0.33);
    font-size: 24px;
    scale: 1.1 1.1;
}
